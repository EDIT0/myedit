<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.privatememo.j">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Privatememo">
        <activity android:name=".ui.OnlyPicFragment"></activity>
        <activity android:name=".ui.CategoryFragment" />
        <activity android:name=".ui.SettingFragment" />
        <activity android:name=".ui.CalendarFragment" />
        <activity android:name=".ui.SearchFragment" />
        <activity android:name=".ui.MemoFragment" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>